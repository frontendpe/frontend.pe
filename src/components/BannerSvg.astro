<script>
  import gsap from "gsap";
  import { ScrollTrigger } from "gsap/ScrollTrigger";
  import { DrawSVGPlugin } from "gsap/DrawSVGPlugin";

  gsap.registerPlugin(ScrollTrigger, DrawSVGPlugin);

  // Wait for DOM so getBBox/getTotalLength work reliably
  const run = () => {
    const svg = document.querySelector("svg");
    if (!svg) return;
    // Global animation flags
    let isAnimatingHouse = false;
    let isAnimatingCar = false;
    let isAnimatingCross = false;
    let isAnimatingLlama = false;
    let isAnimatingTrees = false;
    let isAnimatingPalmer = false;
    let carVelocity = {
      value: 0.01,
      valueMultiplier: 1,
    }; // Car movement speed

    // Select all visual elements
    const elements: SVGElement[] = gsap.utils.toArray(
      svg.querySelectorAll("path, circle"),
    );
    const pathsToAnimate: SVGElement[] = [];

    const svgGroup = {
      car: svg.getElementById("Car"),
      cross: svg.getElementById("Cross"),
      llama: svg.getElementById("Llama"),
      trees: svg.getElementById("Trees"),
      palmer: svg.getElementById("Palmers"),
      houses: svg.getElementById("House"),
    };

    const hitRects = {
      car: svg.getElementById("car-hitbox"),
      cross: svg.getElementById("cross-hitbox"),
      llama: svg.getElementById("llama-hitbox"),
      trees: svg.getElementById("trees-hitbox"),
      palmer: svg.getElementById("palmer-hitbox"),
      house: svg.getElementById("houses-hitbox"),
    };

    // Prepare each element
    elements.forEach((el: SVGElement) => {
      try {
        const computedStyle = window.getComputedStyle(el);
        const fill = computedStyle.getPropertyValue("fill");
        const stroke = computedStyle.getPropertyValue("stroke");

        // We only want to animate elements that are visible (have a fill or a stroke)
        if (
          fill !== "none" ||
          (stroke !== "none" &&
            stroke !== "transparent" &&
            parseFloat(computedStyle.getPropertyValue("stroke-width")) > 0)
        ) {
          pathsToAnimate.push(el);

          // Store original styles
          el.dataset._origFill = fill;
          el.dataset._origFillOpacity =
            computedStyle.getPropertyValue("fill-opacity") ?? "1";
          el.dataset._origStroke = stroke;
          el.dataset._origStrokeWidth =
            computedStyle.getPropertyValue("stroke-width");

          // Set styles for animation
          el.style.fill = "none";
          el.style.stroke = fill !== "none" ? fill : stroke;
          el.style.strokeWidth = "0.5px";
          el.style.strokeLinecap = "round";
          el.style.strokeLinejoin = "round";
        }
      } catch (e) {
        console.error("Could not process element for animation", el, e);
      }
    });
    const tl = gsap.timeline({
      defaults: { ease: "power1.inOut" },
      scrollTrigger: {
        trigger: svg,
        start: "top bottom",
        end: "bottom top",
      },
      onComplete: () => {
        initInteractivity();
      },
    });

    tl.from(pathsToAnimate, {
      duration: 2,
      drawSVG: "0%",
      stagger: {
        each: 0.005,
        from: "random",
      },
    });

    const initInteractivity = () => {
      initCarMove();
      initCarClick();
      initCrossClick();
      initLlamaClick();
      initHouseClick();
      initTreesClick();
      initPalmerClick();
      initCloudAnimation();
    };

    const initCarMove = () => {
      const car = svgGroup.car as SVGGraphicsElement;
      const carHitbox = hitRects.car as SVGGraphicsElement;

      let total = 0;

      // Use viewBox (SVG user units) instead of getBoundingClientRect (CSS pixels).
      // Mixing those causes the wrap range to be too small on scaled/smaller screens.
      const carBBox = car.getBBox();
      const svgViewWidth = svg.viewBox && svg.viewBox.baseVal
        ? svg.viewBox.baseVal.width
        : svg.getBoundingClientRect().width;

      const width = svgViewWidth + carBBox.width;

      const wrap = gsap.utils.wrap(
        0 - carBBox.width * 2 - carBBox.x,
        svgViewWidth - carBBox.x,
      );

      const xTo = gsap.quickTo(car, "x", {
        duration: 1.6,
        ease: "power3.out",
        modifiers: {
          x: gsap.utils.unitize(wrap), // wrap around the stage
        },
      });
      // The car and its hitbox have the same origin, so they can share the same tween.
      const xToHitbox = gsap.quickTo(carHitbox, "x", {
        duration: 1.6,
        ease: "power3.out",
        modifiers: {
          x: gsap.utils.unitize(wrap), // wrap around the stage
        },
      });

      gsap.ticker.add(tick);

      function tick(_: number, dt: number) {
        total -= dt * carVelocity.value * carVelocity.valueMultiplier;
        xTo(total);
        xToHitbox(total);
      }
    };

    const initCarClick = () => {
      const carHitbox = hitRects.car as SVGGraphicsElement;
      carHitbox.addEventListener("click", () => {
        if (isAnimatingCar) return;

        const carTl = gsap.timeline({
          onStart: () => {
            isAnimatingCar = true;
          },
          onComplete: () => {
            isAnimatingCar = false;
          },
        });

        carTl.from(
          carVelocity,
          {
            // scale
            keyframes: [
              { valueMultiplier: 200, ease: "power2.out" },
              { valueMultiplier: 1, ease: "power2.inOut" },
            ],
            duration: 3,
            ease: "power2.inOut",
          },
          0,
        );
      });
    };

    const initCrossClick = () => {
      const crossHitbox = hitRects.cross as SVGGraphicsElement;
      const cross = svgGroup.cross as SVGGraphicsElement;
      crossHitbox.addEventListener("click", () => {
        if (isAnimatingCross) return;

        const crossTl = gsap.timeline({
          onStart: () => {
            isAnimatingCross = true;
          },
          onComplete: () => {
            isAnimatingCross = false;
          },
        });

        const crossPaths = gsap.utils.toArray(cross.querySelectorAll("path"));

        crossTl.from(crossPaths, {
          drawSVG: "0%",
          duration: 1.5,
          stagger: 0.1,
        });
        crossTl.to(
          cross,

          {
            // scale
            keyframes: [
              { scale: 1.5, ease: "power2.out" },
              { scale: 1, ease: "power2.inOut" },
            ],
            transformOrigin: "center bottom",
            duration: 2,
            ease: "power2.inOut",
          },
          1,
        );
      });
    };

    const initLlamaClick = () => {
      const llamaHitbox = hitRects.llama as SVGGraphicsElement;
      const llama = svgGroup.llama as SVGGraphicsElement;
      llamaHitbox.addEventListener("click", () => {
        if (isAnimatingLlama) return;

        const llamaTl = gsap.timeline({
          onStart: () => {
            isAnimatingLlama = true;
          },
          onComplete: () => {
            isAnimatingLlama = false;
          },
        });

        const llamaPaths = gsap.utils.toArray(llama.querySelectorAll("path"));

        llamaTl.from(llamaPaths, {
          drawSVG: "0%",
          fill: "#f7f45f",
          duration: 2,
          stagger: 0.02,
        });
        llamaTl.to(
          llama,
          {
            y: "-20",
            yoyo: true,
            repeat: 5,
            transformOrigin: "center bottom",
            duration: 0.2,
            ease: "none",
          },
          0,
        );
      });
    };

    const initHouseClick = () => {
      const houseHitbox = hitRects.house as SVGGraphicsElement;
      const house = svgGroup.houses as SVGGraphicsElement;
      houseHitbox.addEventListener("click", () => {
        if (isAnimatingHouse) return;

        const houseTl = gsap.timeline({
          onStart: () => {
            isAnimatingHouse = true;
          },
          onComplete: () => {
            isAnimatingHouse = false;
          },
        });

        const housePaths = gsap.utils.toArray(house.querySelectorAll("path"));

        houseTl.from(housePaths, {
          drawSVG: "0%",
          fill: "#38ffe8",
          duration: 2,
          stagger: 0.02,
        });
      });
    };

    const initTreesClick = () => {
      const treesHitbox = hitRects.trees as SVGGraphicsElement;
      const trees = svgGroup.trees as SVGGraphicsElement;
      treesHitbox.addEventListener("click", () => {
        if (isAnimatingTrees) return;

        const treesTl = gsap.timeline({
          onStart: () => {
            isAnimatingTrees = true;
          },
          onComplete: () => {
            isAnimatingTrees = false;
          },
        });

        const treesPaths = gsap.utils.toArray(trees.querySelectorAll("path"));

        treesTl.from(treesPaths, {
          drawSVG: "0%",
          fill: "#f7f45f",
          duration: 2,
          stagger: 0.02,
        });
      });
    };

    const initPalmerClick = () => {
      const palmerHitbox = hitRects.palmer as SVGGraphicsElement;
      const palmer = svgGroup.palmer as SVGGraphicsElement;
      palmerHitbox.addEventListener("click", () => {
        if (isAnimatingPalmer) return;

        const palmerTl = gsap.timeline({
          onStart: () => {
            isAnimatingPalmer = true;
          },
          onComplete: () => {
            isAnimatingPalmer = false;
          },
        });

        const palmerPaths = gsap.utils.toArray(palmer.querySelectorAll("path"));

        palmerTl.from(palmerPaths, {
          drawSVG: "0%",
          fill: "#f7f45f",
          duration: 2,
          stagger: 0.02,
        });
      });
    };

    const initCloudAnimation = () => {
      const clouds: SVGElement[] = gsap.utils.toArray(
        svg.querySelectorAll(".cloud"),
      );
      const svgWidth = svg.viewBox.baseVal.width;

      clouds.forEach((cloud) => {
        const scale = gsap.getProperty(cloud, "scale") as number;
        const cloudBBox = (cloud as SVGGraphicsElement).getBBox();
        const speed = scale * 0.2; // Larger clouds move faster

        gsap.to(cloud, {
          x: `+=${svgWidth + cloudBBox.width}`,
          duration: 20 / speed,
          repeat: -1,
          ease: "none",
          modifiers: {
            x: gsap.utils.unitize(
              (x) =>
                gsap.utils.wrap(
                  -cloudBBox.width * 2,
                  svgWidth + cloudBBox.width * 2,
                )(parseFloat(x)),
              "px",
            ),
          },
        });
      });
    };
  };

  if (
    document.readyState === "complete" ||
    document.readyState === "interactive"
  ) {
    // small timeout to ensure SVG is laid out
    requestAnimationFrame(() => run());
  } else {
    document.addEventListener("DOMContentLoaded", () =>
      requestAnimationFrame(() => run()),
    );
  }
</script>
